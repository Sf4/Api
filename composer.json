{
    "name": "Sf4/Api",
    "type": "library",
    "description": "Simple rest API for Symfony 4",
    "keywords": [
        "Sf4",
        "Api"
    ],
    "homepage": "https://github.com/Sf4/Api",
    "license": "MIT",
    "authors": [
        {
            "name": "Siim Liimand",
            "email": "siim.liimand@gmail.com",
            "homepage": "https://github.com/siimliimand",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1",
        "sf4/populator": "^1.1.1",
        "symfony/framework-bundle": "^4.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Sf4\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sf4\\Api\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
